{
    "name": "vscode-autoimport",
    "displayName": "Auto Import for Javascript",
    "description": "Support ES6 and Flowtype",
    "version": "0.0.56",
    "publisher": "thongdong7",
    "private": true,
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.autoimportRescan",
                "title": "AutoImport re-scan"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "autoimport": "^0.0.56",
        "prettier": "1.6.1",
        "prettier-eslint": "^6.4.2",
        "read-pkg-up": "2.0.0",
        "semver": "^5.4.1"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "@types/semver": "^5.3.31",
        "mocha": "^2.3.3",
        "typescript": "^2.4.2",
        "vscode": "^1.1.5"
    }
}
